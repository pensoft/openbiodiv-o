# inserted as a result of SocialPosition
p-ext:Cleric
      rdf:type owl:Class ;
      rdfs:comment "The situation of holding a position in society as cleric." ;
      rdfs:label "Cleric" ;
      rdfs:subClassOf p-ext:SocialPosition .

     p-ext:hasSocialPositionCleric
        rdf:type owl:ObjectProperty ;
    rdfs:comment "The relation between a person and the social position of Cleric he has. " ;
        rdfs:domain ptop:Person ;
        rdfs:label "has social position cleric" ;
        rdfs:range p-ext:Cleric ;
        owl:inverseOf p-ext:socialPositionCleric ;
    rdfs:subPropertyOf p-ext:socialPositionHolder .

     p-ext:socialPositionCleric
        rdf:type owl:ObjectProperty ;
    rdfs:comment "The relation between a social position of Cleric and the person who has it. " ;
        rdfs:domain p-ext:Cleric ;
        rdfs:label "social position cleric" ;
        rdfs:range ptop:Person ;
        owl:inverseOf p-ext:hasSocialPositionCleric ;
    rdfs:subPropertyOf p-ext:hasSocialPosition .

