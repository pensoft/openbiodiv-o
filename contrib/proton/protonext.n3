      rdf:type owl:Class ;
      rdfs:comment """(http://www.eionet.europa.eu/gemet/concept?ns=1&cp=8401. NIMA GNS designator A.TERR.""" ;
      rdfs:label "Territory" ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:Zone  #DBP3.5
      rdf:type owl:Class ;
      rdfs:comment """(NIMA GNS designator A.ZN, A.ZNB.""" ;
      rdfs:label "Zone" ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:MountainPass     #DBP3.5
      rdf:type owl:Class ;
      rdfs:comment """Any mountain pass. NIMA GNS designators - no direct equivalents""" ;
      rdfs:label "mountain pass"@en ;
      rdfs:subClassOf pupp:LandRegion .

p-ext:AdministrativeRegion     #DBP3.5
      rdf:type owl:Class ;
      rdfs:comment """Any administrative region. NIMA GNS designators - PCLIX""" ;
      rdfs:label "administrative region"@en ;
      rdfs:subClassOf pupp:PopulatedPlace .

