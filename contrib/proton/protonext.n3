  pupp:ISSN
      rdf:type owl:DatatypeProperty ;
      rdfs:comment "The ISSN (International Standard Serial Number) is an eight-digit number which identifies periodical publications as such, including electronic serials. More than one million ISSN numbers have so far been assigned." ;
      rdfs:domain pupp:PeriodicalPublication ;
      rdfs:label "ISSN" ;
