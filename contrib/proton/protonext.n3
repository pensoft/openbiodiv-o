      rdfs:domain pupp:GovernmentOrganization ;
      rdfs:label "of Country" ;
      rdfs:range pupp:Country ;
      rdfs:subPropertyOf ptop:locatedIn .
 pupp:Ministry
      rdf:type owl:Class ;
      rdfs:comment "(Department) A specific ministry or department, structural unit of the government of specific country with focus and authority on specific problems of the country." ;
      rdfs:label "Ministry" ;
      rdfs:subClassOf pupp:GovernmentOrganization .
