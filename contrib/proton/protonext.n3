##      rdfs:subClassOf pupp:PopulatedPlace .

p-ext:Town     #DBP3.5
      rdf:type owl:Class ;
      rdfs:comment """Any town""" ;
      rdfs:label "town"@en ;
      rdfs:subClassOf pupp:PopulatedPlace .

p-ext:Village     #DBP3.5
      rdf:type owl:Class ;
      rdfs:comment """Any village""" ;
      rdfs:label "village"@en ;
      rdfs:subClassOf pupp:PopulatedPlace .

### ldsr4 demo queries for Geonames
p-ext:GeographicRegion  #DBP3.5
      rdf:type owl:Class ;
      rdfs:comment """Any Geographic Region with lattitude and longitude coordinates""" ;
      rdfs:label "geographic region"@en ;
      rdfs:subClassOf ptop:Location .

p-ext:region  #DBP3.5
      rdf:type owl:ObjectProperty ;
      rdfs:comment "any region" ;
      rdfs:domain p-ext:SpatialThing ;
      rdfs:label "region";
      rdfs:range  p-ext:GeographicRegion .

# DBPedia 3.6

p-ext:Arena     #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Any arena""" ;
      rdfs:label "arena"@en ;
      rdfs:subClassOf pupp:Building .  # former pupp:SportFacility . 08032011

p-ext:Atoll     #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Any atoll""" ;
      rdfs:label "atoll"@en ;
      rdfs:subClassOf pupp:Island .

p-ext:BasketballTeam    #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Any basketball team""" ;
      rdfs:label "basketball team"@en ;
      rdfs:subClassOf pupp:Team .

p-ext:Locomotive #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Locomotive, a railway vehicle that provides the motive power for a train. (Wikipedia)""" ;
      rdfs:label "Locomotive" ;
      rdfs:subClassOf pupp:Vehicle .

p-ext:Museum     #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Any museum, an institution that houses and cares for a collection of artifacts and other objects of scientific, artistic, or historical importance and makes them available for public viewing through exhibits that may be permanent or temporary. (Wikipedia)""" ;
      rdfs:label "museum"@en ;
      rdfs:subClassOf pupp:Building .

p-ext:Priest
        # rdf:type owl:Class ;
        rdfs:comment """Any priest, a professsion of a person who is authorized to perform the sacred rites of a religion. (Wikipedia)""" ;
        rdfs:label "priest"@en ;
        rdf:type pupp:Profession .   #how about pupp:JobTitle   ?

p-ext:Restaurant     #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Any restaurant, a place where food, drink and dessert to customers is prepared and served in return for money. (Wikipedia)""" ;
      rdfs:label "restaurant"@en ;
      rdfs:subClassOf pupp:Building .

p-ext:Sculpture     #DBP3.6
      rdf:type owl:Class ;
      rdfs:comment """Any sculpture, three-dimensional artwork created by shaping or combining hard materials. (Wikipedia)""" ;
      rdfs:label "sculpture"@en ;
      rdfs:subClassOf pupp:PieceOfArt .

p-ext:Painting     #added to complete p-ext:Sculpture as a subClassOf PieceOfArt
      rdf:type owl:Class ;
      rdfs:comment """Any painting, the results of a practice of applying paint, pigment, color or other medium[1] to a surface (support base). (Wikipedia) """ ;
      rdfs:label "painting"@en ;
      rdfs:subClassOf pupp:PieceOfArt .

# Geonames 2.2.1

p-ext:Map   #GN 2.2.1
      rdf:type owl:Class ;
      rdfs:comment """A webpage of a map""" ;
      rdfs:label "map"@en ;
      rdfs:subClassOf ptop:Document .

p-ext:locationMap  #GN 2.2.1
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The relation between a geographic region and a map depicting its location" ;
      rdfs:domain p-ext:GeographicRegion ;
      rdfs:label "location map";
      rdfs:range  p-ext:Map .

p-ext:nearby  #GN 2.2.1
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The relation of a location nearby another location" ;
      rdfs:domain p-ext:GeographicRegion ;
      rdfs:label "location nearby another location";
      rdfs:range  p-ext:GeographicRegion .

p-ext:neighbour   #GN 2.2.1
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The relation of a location sharing a common location with another location" ;
      rdfs:domain p-ext:GeographicRegion ;
      rdfs:label "location sharing a common location with another location";
      rdfs:range  p-ext:GeographicRegion .

p-ext:neighbouringGeographicRegions
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The relation between an RDF document containing the descriptions of locations that share a common border" ;
      rdfs:domain p-ext:GeographicRegion ;
      rdfs:label "RDF document containing the descriptions of locations that share a common border";
      rdfs:range  pupp:Dataset .

p-ext:isWrittenAbout
      rdf:type owl:ObjectProperty ;
      rdfs:comment "The relation between an RDF document containing the descriptions of locations that share a common border" ;
      rdfs:domain p-ext:GeographicRegion ;
      rdfs:label "RDF document containing the descriptions of locations that share a common border";
      rdfs:range  pupp:Article .

p-ext:SpatialThing
      rdf:type owl:Class ;
      rdfs:comment """Anything with spatial extent, i.e. size, shape, or position. e.g. people, places, bowling balls, as well as abstract areas like cubes. (from Geonames)""" ;
      rdfs:label "SpatialThing"@en ;
      rdfs:subClassOf ptop:Object .

p-ext:spatialThing
      rdf:type owl:ObjectProperty ;
      rdfs:comment "A spatial thing defined as a geographic region. In Geonames: Type of the feature, as defined in geonames taxonomy. " ;
      rdfs:domain p-ext:SpatialThing ;
      rdfs:label "Spatial thing - geographic region";
      rdfs:range  p-ext:GeographicRegion .


## Codes from Geonames v 2.2.1

p-ext:Bight
      rdf:type owl:Class ;
      rdfs:comment """an open body of water forming a slight recession in a coastline""" ;
      rdfs:label "bight"@en ;
      rdfs:subClassOf pupp:LandRegion .

p-ext:Icecap
      rdf:type owl:Class ;
      rdfs:comment """a dome-shaped mass of glacial ice covering an area of mountain summits or other high lands; smaller than an ice sheet""" ;
      rdfs:label "icecap"@en ;
      rdfs:subClassOf pupp:LandRegion .

p-ext:MudFlat
      rdf:type owl:Class ;
      rdfs:comment """ a relatively level area of mud either between high and low tide lines, or subject to flooding; a large flat area of mud or sand attached to the shore and alternately covered and uncovered by the tide; a tropical tidal mud flat characterized by mangrove vegetation   """ ;
      rdfs:label "mud flat"@en ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:Glacier
      rdf:type owl:Class ;
      rdfs:comment """ a mass of ice, usually at high latitudes or high elevations, with sufficient thickness to flow away from the source area in lobes, tongues, or masses  """ ;
      rdfs:label "glacier"@en ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:FInlet
      rdf:type owl:Class ;
      rdfs:comment """ an inlet which has been filled in, or blocked by deposits """ ;
      rdfs:label "former inlet"@en ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:Marsh
      rdf:type owl:Class ;
      rdfs:comment """ an area of open ground overlaid with wet peaty soils; a wetland dominated by grass-like vegetation; a flat area, subject to periodic salt water inundation, dominated by grassy salt-tolerant plants; a salt flat or salt encrusted plain subject to periodic inundation from flooding or high tides """ ;
      rdfs:label "marsh"@en ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:GovernmentBuilding
      rdf:type owl:Class ;
      rdfs:comment """ a government building, a building which serve the government, or where government officials work. """ ;
      rdfs:label "government building"@en ;
      rdfs:subClassOf pupp:Building .

p-ext:AgriculturalFacility
      rdf:type owl:Class ;
      rdfs:comment """ agricultural facility; a building and/or tract of land used for improving agriculture """ ;
      rdfs:label "agricultural facility"@en ;
      rdfs:subClassOf pupp:Facility .

p-ext:Airbase
      rdf:type owl:Class ;
      rdfs:comment """ an area used to store supplies, provide barracks for air force personnel, hangars and runways for aircraft, and from which operations are initiated """ ;
      rdfs:label "airbase"@en ;
      rdfs:subClassOf pupp:MilitaryAreas .

p-ext:Arch
      rdf:type owl:Class ;
      rdfs:comment """ a natural or man-made structure in the form of an arch """ ;
      rdfs:label "arch"@en ;
      rdfs:subClassOf pupp:NonGeographicLocation .

p-ext:ATM
      rdf:type owl:Class ;
      rdfs:comment """ An unattended electronic machine in a public place, connected to a data system and related equipment and activated by a bank customer to obtain cash withdrawals and other banking services. """ ;
      rdfs:label "automatic teller machine (ATM)"@en ;
      rdfs:subClassOf pupp:TelecomFacility .

p-ext:Bank
      rdf:type owl:Class ;
      rdfs:comment """ A business establishment in which money is kept for saving or commercial purposes or is invested, supplied for loans, or exchanged. """ ;
      rdfs:label "bank"@en ;
      rdfs:subClassOf pupp:Building .

p-ext:Beacon
      rdf:type owl:Class ;
      rdfs:comment """ a fixed artificial navigation mark """ ;
      rdfs:label "beacon"@en ;
      rdfs:subClassOf pupp:Facility .

p-ext:Barracks
      rdf:type owl:Class ;
      rdfs:comment """ a building for lodging military personnel """ ;
      rdfs:label "barracks"@en ;
      rdfs:subClassOf pupp:MilitaryAreas .

p-ext:Breakwater
      rdf:type owl:Class ;
      rdfs:comment """ a structure erected to break the force of waves at the entrance to a harbor or port """ ;
      rdfs:label "breakwater"@en ;
      rdfs:subClassOf pupp:Facility .

